{
	"name": "tHaven",
  "version": "1.18.0",
  "description": "基于tauri的wallhaven",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vite build",
    "prepare": "husky install",
    "dev:host": "vite --host",
    "dev:open": "vite --open",
    "preview": "vite preview",
    "coverage": "vitest --coverage",
    "preinstall": "npx only-allow pnpm",
    "typecheck": "npx vue-tsc --noEmit",
    "preview:host": "vite preview --host",
    "preview:open": "vite preview --open",
    "lint": "eslint --ext .ts,.js,.vue .",
		"lint:fix": "eslint --fix",
    "build:debug": "cross-env NODE_ENV=debug vite build"
  },
  "engines": {
    "node": ">=v18.17.0"
  },
  "packageManager": "pnpm@8.7.0",
  "devDependencies": {
		"@antfu/eslint-config": "^1.1.0",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@tauri-apps/api": "^1.5.1",
		"@types/ityped": "^1.0.3",
    "@types/node": "^18.17.12",
		"@typescript-eslint/parser": "^6.10.0",
		"@unocss/eslint-config": "^0.57.2",
		"@unocss/reset": "^0.57.2",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vueuse/components": "^10.5.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/integrations": "^10.5.0",
    "changelogen": "^0.5.5",
    "cross-env": "^7.0.3",
		"eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "ityped": "^1.0.3",
		"lightningcss": "^1.22.1",
    "lint-staged": "^15.0.2",
    "local-pkg": "^0.5.0",
    "naive-ui": "^2.35.0",
    "perfect-debounce": "^1.0.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "typescript": "^5.2.2",
		"unocss": "^0.57.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^4.5.0",
    "vite-auto-import-resolvers": "^3.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-types": "^0.1.3",
    "vite-plugin-removelog": "^0.2.2",
    "vite-plugin-use-modules": "^1.4.5",
    "vite-plugin-vue-meta-layouts": "^0.3.1",
    "vite-plugin-warmup": "^0.1.0",
		"vue": "^3.3.8",
		"vue-dark-switch": "^1.0.6",
		"vue-i18n": "^9.6.5",
    "vue-request": "2.0.4",
    "vue-router": "^4.2.5",
    "vue-toastification": "2.0.0-rc.5"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix"
  },
  "overrides": {
    "vue-demi": "npm:vue-demi@latest",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@latest",
